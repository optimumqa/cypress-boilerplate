{
  "name": "cy-boilerplate",
  "version": "0.0.1",
  "scripts": {
    "add-project": "hygen project with-prompt && prettier --write . >/dev/null",
    "clean-reports": "rm -rf cypress/reports && mkdir -p cypress/reports/mochareports",
    "pretest": "npm run clean-reports",
    "combine-reports": "mochawesome-merge cypress/reports/mocha/*.json > cypress/reports/mochareports/report.json",
    "generate-report": "marge cypress/reports/mochareports/report.json -f index -o cypress/reports/mochareports/",
    "posttest": "npm run combine-reports && npm run generate-report",
    "assets.clear": "rm -rf cypress/screenshots cypress/videos",
    "prettify": "npx prettier --write .",
    "prepare": "husky install"
  },
  "dependencies": {
    "@optimumqa/cypress-setup-utilities": "1.5.1",
    "@optimumqa/cypress-store": "1.0.2",
    "cypress": "12.11.0",
    "cypress-iframe": "1.0.1",
    "cypress-localstorage-commands": "2.2.3",
    "cypress-multi-reporters": "1.6.3",
    "eslint": "8.39.0",
    "eslint-plugin-cypress": "2.13.2",
    "hygen": "6.2.11",
    "mocha": "10.2.0",
    "mochawesome": "7.1.3",
    "mochawesome-merge": "4.3.0",
    "mochawesome-report-generator": "6.2.0",
    "typescript": "5.0.4",
    "del": "6.1.1",
    "fs-extra": "^11.1.0",
    "path": "^0.12.7",
    "husky": "^8.0.3",
    "prettier": "2.8.4"
  }
}
